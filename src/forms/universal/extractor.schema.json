{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://example.com/schemas/universal/extractor.schema.json",
  "title": "Universal U.S. Real Estate Transaction Extraction (Per-Page)",
  "description": "Version 4.0.0 - 2025-12-30 - UPDATED: Examples instead of descriptions, aligned with ParseResult types",
  "type": "array",
  "minItems": 1,
  "items": {
    "type": "object",
    "properties": {
      "pageNumber": {
        "type": "integer",
        "minimum": 1,
        "example": 1
      },
      "pageLabel": {
        "type": "string",
        "example": "RPA PAGE 1 - TRANSACTION TERMS"
      },
      "formCode": {
        "type": "string",
        "example": "RPA"
      },
      "formPage": {
        "type": ["integer", "null"],
        "example": 1
      },
      "pageRole": {
        "type": "string",
        "enum": ["main_contract", "counter_offer", "addendum", "signatures", "broker_info"],
        "example": "main_contract"
      },
      "buyerNames": {
        "type": ["array", "null"],
        "items": { "type": "string", "minLength": 1 },
        "minItems": 1,
        "maxItems": 6,
        "example": ["John Smith", "Jane Doe"]
      },
      "sellerNames": {
        "type": ["array", "null"],
        "items": { "type": "string" },
        "minItems": 1,
        "maxItems": 6,
        "example": ["Robert Johnson"]
      },
      "propertyAddress": {
        "type": ["string", "null"],
        "minLength": 10,
        "example": "123 Main Street, Los Angeles, CA 90210"
      },
      "purchasePrice": {
        "type": ["number", "null"],
        "minimum": 0,
        "example": 750000
      },
      "earnestMoneyDeposit": {
        "type": ["object", "null"],
        "properties": {
          "amount": { "type": ["number", "null"], "example": 15000 },
          "holder": { "type": ["string", "null"], "example": "First American Title Company" }
        },
        "example": { "amount": 15000, "holder": "First American Title Company" }
      },
      "closingDate": {
        "oneOf": [
          { "type": "string", "example": "2025-03-15" },
          { "type": "integer", "example": 45 },
          { "type": "null" }
        ]
      },
      "financing": {
        "type": ["object", "null"],
        "properties": {
          "isAllCash": { "type": "boolean", "example": false },
          "loanType": {
            "type": ["string", "null"],
            "enum": ["Conventional", "FHA", "VA", "USDA", "Other", null],
            "example": "Conventional"
          },
          "loanAmount": { "type": ["number", "null"], "example": 600000 }
        },
        "required": ["isAllCash"],
        "example": { "isAllCash": false, "loanType": "Conventional", "loanAmount": 600000 }
      },
      "contingencies": {
        "type": ["object", "null"],
        "properties": {
          "inspectionDays": {
            "oneOf": [
              { "type": "integer", "example": 17 },
              { "type": "string", "example": "Waived" },
              { "type": "null" }
            ]
          },
          "appraisalDays": {
            "oneOf": [
              { "type": "integer", "example": 17 },
              { "type": "string", "example": "2025-02-15" },
              { "type": "null" }
            ]
          },
          "loanDays": {
            "oneOf": [
              { "type": "integer", "example": 21 },
              { "type": "string", "example": "Waived" },
              { "type": "null" }
            ]
          },
          "saleOfBuyerProperty": { "type": "boolean", "example": false }
        },
        "required": ["saleOfBuyerProperty"],
        "example": { "inspectionDays": 17, "appraisalDays": 17, "loanDays": 21, "saleOfBuyerProperty": false }
      },
      "closingCosts": {
        "type": ["object", "null"],
        "properties": {
          "buyerPays": { "type": "array", "items": { "type": "string" }, "example": ["title", "escrow"] },
          "sellerPays": { "type": "array", "items": { "type": "string" }, "example": ["transfer tax"] },
          "sellerCreditAmount": { "type": ["number", "null"], "example": 5000 }
        },
        "required": ["buyerPays", "sellerPays"],
        "example": { "buyerPays": ["title", "escrow"], "sellerPays": ["transfer tax"], "sellerCreditAmount": 5000 }
      },
      "brokers": {
        "type": ["object", "null"],
        "properties": {
          "listingBrokerage": { "type": ["string", "null"], "example": "Coldwell Banker" },
          "listingAgent": { "type": ["string", "null"], "example": "Sarah Johnson" },
          "sellingBrokerage": { "type": ["string", "null"], "example": "Keller Williams" },
          "sellingAgent": { "type": ["string", "null"], "example": "Michael Chen" }
        },
        "example": { "listingBrokerage": "Coldwell Banker", "listingAgent": "Sarah Johnson", "sellingBrokerage": "Keller Williams", "sellingAgent": "Michael Chen" }
      },
      "personalPropertyIncluded": {
        "type": "array",
        "items": { "type": "string" },
        "example": ["Refrigerator", "Washer", "Dryer", "Window coverings"]
      },
      "effectiveDate": {
        "type": ["string", "null"],
        "format": "date",
        "example": "2025-01-15"
      },
      "escrowHolder": {
        "type": ["string", "null"],
        "example": "Chicago Title Company"
      },
      "confidence": {
        "type": "object",
        "properties": {
          "overall": {
            "type": "integer",
            "minimum": 0,
            "maximum": 100,
            "example": 95
          },
          "fieldScores": {
            "type": ["object", "null"],
            "additionalProperties": {
              "type": "integer",
              "minimum": 0,
              "maximum": 100
            },
            "example": { "propertyAddress": 100, "purchasePrice": 98, "buyerNames": 95 }
          }
        },
        "required": ["overall"],
        "example": { "overall": 95, "fieldScores": { "propertyAddress": 100, "purchasePrice": 98 } }
      }
    },
    "required": [
      "pageNumber",
      "pageLabel",
      "formCode",
      "pageRole",
      "confidence"
    ],
    "additionalProperties": false
  }
}